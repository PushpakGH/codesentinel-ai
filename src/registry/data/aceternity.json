  "components": [
    {
      "name": "3d-card",
      "category": "interactive",
      "description": "Card with 3D tilt and parallax effect on mouse hover.",
      "dependencies": ["framer-motion"],
      "props": {
        "className": {
          "type": "string",
          "description": "Tailwind classes for the inner card container."
        },
        "containerClassName": {
          "type": "string",
          "description": "Tailwind classes for the outer container/wrapper."
        }
      },
      "usage": "import { CardContainer, CardBody } from \"@/components/aceternity/3d-card\"",
      "examples": [
        {
          "title": "3D Product Card",
          "code": "<CardContainer className=\"w-full max-w-sm\">\n  <CardBody className=\"rounded-2xl border bg-card p-6 shadow-lg\">\n    <h3 className=\"text-lg font-semibold\">Pro Plan</h3>\n    <p className=\"mt-2 text-sm text-muted-foreground\">Perfect for growing teams.</p>\n  </CardBody>\n</CardContainer>"
        }
      ]
    },
    {
      "name": "3d-pin",
      "category": "interactive",
      "description": "3D animated gradient pin used to highlight links or cards.",
      "dependencies": ["framer-motion"],
      "props": {
        "href": {
          "type": "string",
          "description": "Optional link for the pinned item."
