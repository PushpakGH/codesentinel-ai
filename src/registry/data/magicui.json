    {
      "name": "animated-beam",
      "category": "animations",
      "description": "Animated connection lines between elements with smooth transitions.",
      "dependencies": [
        "framer-motion"
      ],
      "props": {
        "from": {
          "type": "HTMLElement | null",
          "description": "Source element for the beam line."
        },
        "to": {
          "type": "HTMLElement | null",
          "description": "Target element for the beam line."
        },
        "duration": {
          "type": "number",
          "default": 1.2,
          "description": "Animation duration in seconds."
        }
      },
      "usage": "import { AnimatedBeam } from \"@/components/magicui/animated-beam\"",
      "examples": [
        {
          "title": "Connect Two Cards",
          "code": "<AnimatedBeam from={cardRefA.current} to={cardRefB.current} />"
        }
      ],
      "tags": [
        "beam",
        "connector",
        "line",
        "flow"
      ],
      "useCase": "Connecting two elements with a moving beam, good for explaining data flow."
    },
    {
      "name": "animated-list",
